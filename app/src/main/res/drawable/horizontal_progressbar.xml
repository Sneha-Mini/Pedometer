<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="5dip" />
            <solid android:color="#CACACA"></solid>
        </shape>
    </item>

    <item>
        <clip>
            <shape>
                <corners android:radius="5dip" />
